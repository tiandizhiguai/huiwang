#if($!{datas.size()} > 0)
	<link rel="stylesheet" href="//style.1huiwang.com/huiwang/css/admin/adminList.css"/>
	<table class="table table-bordered">
	   <thead>
	      <tr>
	         <th>标题</th>
	         <th>阅读次数</th>
	         <th>关注人数</th>
	         <th>赞人数</th>
	         <th>收藏人数</th>
	         <th>转发次数</th>
	         <th>回复次数</th>
	         <th>发布日期</th>
	         <th>操作</th>
	      </tr>
	   </thead>
	   <tbody>
	       #foreach($data in $!datas)
	           <tr class="active">
	              <td title="$!{data.title}">$!{data.getTitle(10)}</td>
	              <td>$!{data.statisData.readSize}</td>
	              <td>$!{data.statisData.careSize}</td>
	              <td>$!{data.statisData.praiseSize}</td>
	              <td>$!{data.statisData.favoriteSize}</td>
	              <td>$!{data.statisData.forwardSize}</td>
	              <td>$!{data.statisData.commentSize}</td>
	              <td>$!{data.gmtCreated}</td>
	              <td>
	                  <a href="#" class="pre_edit_action" id="$!{data.id}">修改</a>|
	                  <a href="#" class="pre_delete_action" id="$!{data.id}">删除</a>
	              </td>
	           </tr>
	      #end
	   </tbody>
	</table>
	#parse("common/paging.vm")
	<script src="//style.1huiwang.com/huiwang/js/admin/adminArticles.js"></script>
#else
	<div class="admin_welcome">
         还没有文章哦，去发布吧
    </div>
#end