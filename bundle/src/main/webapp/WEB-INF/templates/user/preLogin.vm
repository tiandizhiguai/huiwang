<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>首页</title>
	    #parse("common/common.vm")
	</head>
	<body>
	    <div>
		    #parse("common/header.vm")
		    <link rel="stylesheet" href="//style.rensheng.com/rensheng/css/preRegister.css"/>
		    <div class="preRegister">
				<form class="pre_login_form" action="//www.rensheng.com/user/login" method="post">
				  <input type="hidden" value="$!{redirectUri}" name="redirectUri"/>
			      <table>
			          <tr>
						  <td class="company_logo"><strong>回望</strong></td>
						  <td></td>
				      </tr>
					  <tr>
						  <td>
						      <div class="input-group">
						         <input type="text" class="form-control" name="loginName" id="loginName" placeholder="登陆名">
						      </div>
					      </td>
					      <td><span class="tips" id="check_user"></span></td>
				      </tr>
					  <tr>
						  <td>
						      <div class="input-group">
						         <input type="password" class="form-control" name="passwd" id="passwd" placeholder="密码">
						      </div>
					      </td>
					      <td><span class="tips" id="check_passwd"></span></td>
				      </tr>
				      <tr>
					      <td>
					          <button type="button" class="btn btn-primary register_login_btn" id="pre_login_btn">登陆</button>
					      </td>
				      </tr>
			      </table>
			    </form>
		    </div>
		    <script src="//style.rensheng.com/rensheng/js/preLogin.js"></script>
		    #parse("common/footer.vm")
		</div>
	</body>
</html>