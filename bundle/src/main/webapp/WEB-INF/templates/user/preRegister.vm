<!DOCTYPE html>
<html>
	<head>
		#parse("common/head.vm")
	</head>
	<body>
	    <div>
		    #parse("common/header.vm")
		    <link rel="stylesheet" href="//style.1huiwang.com/huiwang/css/preRegister.css"/>
		    <div class="preRegister">
				<form class="pre_register_form" action="//www.1huiwang.com/user/register" method="post">
				  <table>
				  	  <tr>
						  <td class="company_logo"><strong>回望</strong></td>
						  <td></td>
				      </tr>
					  <tr>
						  <td>
						      <div class="input-group">
						         <input type="text" class="form-control" name="loginName" id="loginName" placeholder="登陆名">
						      </div>
					      </td>
					      <td align="left"><span class="tips" id="check_user"></span></td>
				      </tr>
					  <tr>
						  <td>
						      <div class="input-group">
						         <input type="password" class="form-control" name="passwd" id="passwd" placeholder="密码">
						      </div>
					      </td>
					      <td align="left"><span class="tips" id="check_passwd"></span></td>
				      </tr>
				      <tr>
					      <td>
						      <div class="input-group">
						         <input type="password" class="form-control" name="rePasswd" id="rePasswd" placeholder="确认密码">
						      </div>
					      </td>
				          <td align="left"><span class="tips" id="check_repasswd"></span></td>
				      </tr>
				      <tr>
					      <td>
					          <button type="button" class="btn btn-primary register_login_btn" id="pre_register_btn">注册</button>
					      </td>
				      </tr>
			      </table>
			      <input type="hidden" value="$!{tokenValue}" name="token"/>
			    </form>
		    </div>
		    <script src="//style.1huiwang.com/huiwang/js/preRegister.js"></script>
		    #parse("common/footer.vm")
		</div>
	</body>
</html>