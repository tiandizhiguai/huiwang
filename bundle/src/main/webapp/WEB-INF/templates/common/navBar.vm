<link rel="stylesheet" href="//style.rensheng.com/rensheng/css/navBar.css"/>
<div class="nav_bar">
    <ul class="nav_bar_items">
    	#foreach($topicData in $!topicDatas)
    		<li class="nav_bar_item #if($selectedTopicId == $!{topicData.id}) bar_hover#end">
    			<a href="//www.rensheng.com/index?topicId=$!{topicData.id}">$!{topicData.name}</a>
    		</li>
    	#end
    </ul>
</div>
<script src="//style.rensheng.com/rensheng/js/navBar.js"></script>